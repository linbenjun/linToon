				Gradient gradient83 = NewGradient( 1, 8, 2, float4( 0, 0, 0, 0.1286336 ), float4( 1, 1, 1, 0.1941711 ), float4( 0, 0, 0, 0.4733043 ), float4( 1, 1, 1, 0.6456245 ), float4( 0, 0, 0, 0.8228123 ), float4( 1, 1, 1, 0.8592203 ), float4( 0, 0, 0, 0.9878691 ), float4( 1, 1, 1, 0.9975738 ), float2( 1, 0.004852369 ), float2( 1, 1 ), 0, 0, 0, 0, 0, 0 );
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = normalize(ase_worldViewDir);
				half3 normalizeResult79 = normalize( ( ase_worldViewDir + _MainLightPosition.xyz ) );
				half3 ase_worldNormal = IN.ase_texcoord3.xyz;
				half dotResult77 = dot( normalizeResult79 , ase_worldNormal );
				float2 uv_Texture = IN.ase_texcoord4.xy * _Texture_ST.xy + _Texture_ST.zw;
				half4 tex2DNode7 = tex2D( _Texture, uv_Texture );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( ( SampleGradient( gradient83, dotResult77 ).r * 0.4 ) + tex2DNode7 ).rgb;
				float Alpha = ( tex2DNode7.a * 0.68 );
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
